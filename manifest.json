{
  "manifest_version": 2,

  "name": "CTM-logs-console",
  "description": "A chrome extension so you don't have to go hunt for VersionOne Continuum errors",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click for CTM logs"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "http://localhost:8080/"
  ]
}
