{
  "manifest_version": 2,

  "name": "Continuum Logs devtools",
  "description": "Get the VersionOne Continuum logs right in your devtools",
  "version": "1.0",
  "devtools_page": "devtools.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}
